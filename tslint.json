{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "max-line-length": false,
        "semicolon": [true, "never"],
        "no-empty": false,
        "no-console": false,
        "quotemark": [true, "single"],
        "no-bitwise": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ],
        "variable-name": false,
        "prefer-for-of": false
    },
    "rulesDirectory": []
}
