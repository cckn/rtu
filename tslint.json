{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "max-line-length": [true, 140],
        "semicolon": [true, "never"],
        "no-empty": false,
        "no-console": false,
        "quotemark": [true, "single"],
        "no-bitwise": false
    },
    "rulesDirectory": []
}
